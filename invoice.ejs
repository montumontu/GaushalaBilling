<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html>
  <head>
    <style>
      body {
      }

      #doctitle {
        width: 40%;
        height: 25px;
        text-align: center;
      }
      #orgname {
        
      }
      table {
        border-collapse: collapse;
      }
      td, th {
        border: 1px solid black;
        padding: 5px;
        text-align: center;
      }

      table tr td {
        padding: 1px;
      }

      #daytoday {
       
      }

      #daytodaymilk {
        font-size:8px;
      }
      #daytodaymilk tr{
        
      }
    </style>
    <link rel="stylesheet" href="/invoice.css" type="text/css"></style>
      <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  </head>
  <body>
    <div style="width: 670px; float: left;">
      <div id="doctitle">
      </div>
      <div>
        <img src="data:image/png;base64," style="float: left; height: 100px; "/>
        <h1 id="orgname" style="float: left; margin-top: 40px;">Neamatpur Gir Gaushala</h1>
      </div>
      <div>
        <table border= "1"  style=" float: left; width: 670px">
          <tr>
            <th>Date & Time</th>
            <th rowspan="2">Sodepur Village Main Rd, Sodepur, West Bengal 713360</th>
            <th>GoSeva Rasi id</th>
          </tr>
          <tr>
            <td><%=currentDate%></td>
            <td><%=randomNumber%></td>
          </tr>
        </table>
        <table border="1" style=" width: 670px">
          <tr>
            <th>Name</th>
            <td><%=data[0]%></td>
          </tr>
          <tr>
            <th>Address</th>
            <td><%=data[41]%></td>
          </tr>
          <tr>
            <th>Mobile Number</th>
            <td><%=data[42]%></td>
          </tr>
          <tr>
            <th>Month</th>
            <td><%=month%></td>
          </tr>
          <tr>
            <th>Email</th>
            <td><%=data[43]%></td>
          </tr>
        </table>
        <table border="1" style=" width: 670px">
          <tr>
            <th>SI.No.</th>
            <th>Product</th>
            <th>Price</th>
            <th>Quantity</th>
            <th>Amount</th>
          </tr>
          <tr>
            <td>1</td>
            <td>Gir Cow Milk</td>
            <td><%=data[36]%></td>
            <td><%=data[33]%></td>
            <td><%=data[33]*data[36]%></td>
          </tr>
          <tr>
            <td>2</td>
            <td>Delivery Charges</td>
            <td><%=data[38]%></td>
            <td><%=data[37]%></td>
            <td><%=data[39]%></td>
          </tr>
        </table>
        <table border="1">
          <tr>
            <th rowspan="6" style="width: 470px">
          </tr>
          <!-- <tr>
            <th>TOTAL QUANTITY</th>
            <td><%=data[40]%></td>
          </tr> -->
          <tr>
            <th>TOTAL AMOUNT</th>
            <td><%=data[40]%></td>
          </tr>
          <tr>
            <th>DUE</th>
            <td>0</td>
          </tr>
          <tr>
            <th>DISCOUNT</th>
            <td>0</td>
          </tr>
          <tr>
            <th>FINAL AMOUNT</th>
            <td><%=data[40]%></td>
          </tr>
        </table>
        <table style=" width: 670px" border="1">
          <tr>
            <td>PH NO - 9474309855, 7908102415 Email- info.gauras@gmail.com </td></tr>
        </table>
      </div>
    </div>
    <div id= "daytoday" style="margin-top: 120px; height: 1000px; float: left; width: 200px; padding: 5px;">
      <table id="daytodaymilk" border="1" style="text-align: center; font-size: 8px;">
        <tr>
          <th>Day</th>
          <th>Litre</th>
        </tr>
        <% for (var i = 1; i <= 31 ; i++) { %>
          <tr>
            <td><%=i%></td>
            <td><%=data[i]%></td>
          </tr>
        <% } %>
        <tr>
          <th>
            Total
          </th>
          <th>
            30
          </th>
        </tr>
      </table>
    </div>
  </body>
</html>
